{
    "name": "vite-vue3-typescript-eslint-prettier",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "run-p dev-ts dev-vite",
        "dev-ts": "vue-tsc --watch --noEmit",
        "dev-vite": "vite",
        "build": "vue-tsc && vite build",
        "lint": "eslint \"src/**/*.vue\" \"src/**/*.ts\"",
        "format": "prettier --write \"src/**/*.vue\" \"src/**/*.ts\"",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "icons-generate-names": "node ./scripts/generateIconNames.mjs",
        "icons-cleanup-svg": "svgo --config ./svgo.config.cjs -f ./src/assets/icons -o ./src/assets/icons/optimised"
    },
    "dependencies": {
        "latest": "^0.2.0",
        "pinia": "^2.1.7",
        "vite-svg-loader": "^4.0.0",
        "vue": "^3.4.32",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.4.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.6.20",
        "@storybook/addon-interactions": "^7.6.20",
        "@storybook/addon-links": "^7.6.20",
        "@storybook/blocks": "^7.6.20",
        "@storybook/vue3": "^7.6.20",
        "@storybook/vue3-vite": "^7.6.20",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/eslint-config-typescript": "^11.0.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-no-relative-import-paths": "^1.5.5",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-unused-imports": "^3.2.0",
        "eslint-plugin-vue": "^9.27.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sass": "^1.77.8",
        "storybook": "^7.6.20",
        "svgo": "^3.3.2",
        "typescript": "^5.5.3",
        "vite": "^4.5.3",
        "vite-plugin-eslint": "^1.8.1",
        "vue-tsc": "2.0.26"
    }
}
